{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"*": ["node_modules/*", "*"]
		},
		"allowJs": true,
		"checkJs": false,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"target": "es5"
	},
	"exclude": [
		"Docs",
		"Documentation",
		"node_modules",
		".git",
		"Release"
	]
}
